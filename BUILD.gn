  install_enable = true
  subsystem_name = "${THIRDPARTY_GRPC_SUBSYS_NAME}"
  part_name = "${THIRDPARTY_GRPC_PART_NAME}"
}
config("upb_config") {
  include_dirs = [
    "${GRPC_DIR}/third_party/upb/",

import("//build/ohos.gni")
THIRDPARTY_GRPC_SUBSYS_NAME = "thirdparty"
THIRDPARTY_GRPC_PART_NAME = "grpc"
libgrpc_path = rebase_path("//third_party/grpc")
exec_script("install.sh", [ "$libgrpc_path" ])

GRPC_DIR = rebase_path("//third_party/grpc/grpc")

config("pulbic_grpc_config") {
  include_dirs = [
    "${GRPC_DIR}/",
    "${GRPC_DIR}/include/",
    "//third_party/abseil-cpp/abseil-cpp",
    "//third_party/re2/re2",
  ]
  defines = [
    "GRPC_USE_PROTO_LITE",
    "GPR_SUPPORT_CHANNELS_FROM_FD",
    "GRPC_ARES=0",
    "GPR_PTHREAD_TLS",
  ]
}

config("private_grpc_config") {
  visibility = [ ":*" ]
  cflags = [
    "-Wno-implicit-fallthrough",
    "-Wno-unused-variable",
    "-Wno-ignored-qualifiers",
    "-Wno-atomic-implicit-seq-cst",
    "-Wno-undef",
    "-Wno-missing-prototypes",
    "-Wno-missing-variable-declarations",
    "-Wno-sign-compare",
    "-Wno-shadow-uncaptured-local",
    "-Wno-deprecated-builtins",
    "-Wno-error=unused-variable",
  ]
  defines = [ "OPENSSL_SUPPRESS_DEPRECATED" ]
}

ohos_shared_library("address_sorting") {
  sources = [
    "${GRPC_DIR}/third_party/address_sorting/address_sorting.c",
